{
  "version": "1.0.0",
  "name": "@carbon/database",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "db:init": "supabase init",
    "db:start": "supabase start",
    "db:kill": "supabase stop",
    "db:types": "supabase gen types typescript --local > src/types.ts",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:new": "prisma migrate dev --create-only",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:seed": "tsx src/seed.ts",
    "db:migrate:force": "prisma migrate reset --force",
    "dev": "tsup --watch",
    "format": "prisma format",
    "generate": "prisma generate",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "setup": "run-s setup:db db:seed",
    "setup:db": "prisma generate && prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^4.6.0"
  },
  "devDependencies": {
    "@carbon/tsconfig": "*",
    "@supabase/supabase-js": "^2.1.0",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "dotenv": "16.0.3",
    "eslint": "^8.4.1",
    "eslint-config-carbon": "*",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.6.0",
    "rimraf": "^3.0.2",
    "supabase": "^1.16.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3",
    "uuid": "9.0.0"
  }
}
