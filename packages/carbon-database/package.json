{
  "version": "0.0.1",
  "name": "@carbon/database",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "db:init": "supabase init",
    "db:start": "supabase start",
    "db:kill": "supabase stop",
    "db:types": "supabase gen types typescript --local > src/types.ts",
    "db:migrate": "supabase db reset",
    "db:seed": "tsx src/seed.ts",
    "db:setup": "supabase db reset",
    "dev": "tsup --watch",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "setup": "run-s db:setup db:seed",
    "deploy": "supabase link --project-ref $SUPABASE_PROJECT_ID && supabase db push"
  },
  "devDependencies": {
    "@carbon/tsconfig": "*",
    "@supabase/supabase-js": "^2.8.0",
    "@types/node": "^16.17.1",
    "@types/uuid": "^8.3.4",
    "dotenv": "16.0.3",
    "eslint": "^8.4.1",
    "eslint-config-carbon": "*",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supabase": "^1.27.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3",
    "uuid": "9.0.0"
  }
}
