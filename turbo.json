{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "build/**", "api/**", "public/build/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "db:start": {
      "outputs": []
    },
    "db:kill": {
      "outputs": []
    },
    "db:types": {
      "outputs": [],
      "cache": true
    },
    "db:init": {
      "outputs": []
    },
    "db:migrate": {
      "outputs": [],
      "cache": false
    },
    "db:push": {
      "outputs": []
    },
    "db:pull": {
      "outputs": []
    },
    "db:seed": {
      "outputs": [],
      "cache": false
    },
    "db:migrate:force": {
      "outputs": []
    },
    "setup": {
      "cache": false,
      "outputs": []
    }
  }
}
